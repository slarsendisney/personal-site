  "scripts": {
    "build": "gatsby build",
    "build:nike": "npm run nike_data_update && gatsby build",
    "count": "cloc  --by-file --exclude-ext=json,svg src/ MDX/",
    "count_totals": "cloc --exclude-ext=svg --exclude-dir=animation-data --json src/ MDX/ > data/stats/count_total.json",
    "count_commits": "git log --all --pretty=format:'%n{%n%d%n  \"commit hash\": \"%H\",%n  \"date\": \"%ad\",%n  \"message\": \"%f\"%n}'| grep -v \"origin\" | tr -d '\n' | sed 's/}{/}, {/g' | sed -e 's/^/[/' |sed -e 's/$/]/' > data/stats/gitlog.json",
    "postinstall": "npm run count_totals",
    "develop": "gatsby develop -o",
    "deploy": "gatsby build && firebase deploy",
    "update": "ncu -u  && npm install",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "nike_activity": "./scripts/getActivity.sh",
    "nike_data_update": "node ./scripts/getNikeData",
    "start": "npm run hello && npm run count_totals && npm run develop",
    "serve": "gatsby serve",
    "hello": "node scripts/hello.js",
    "fix-styles": "npx stylelint 'src/styles/**/*.scss' --syntax scss --fix"
  },
  "repository": {
    "type": "git",
